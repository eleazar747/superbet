          <table class="table table-condensed table-hover table-striped">
				<thead>
					<tr>
						[[#model.metaProperties]]
						<th>
							[[propertyName]]
						</th>
						[[/model.metaProperties]]
						<th>Action</th>
					</tr>
				</thead>
				<tbody>
				{{#entity}}
					<tr>
						[[#model.metaProperties]]
						<td>
							{{[[property]]}}
						</td>
						[[/model.metaProperties]]
						<td><button type="submit" class="btn btn-danger" onclick="javascript:alert({{id}})">Delete</button></td>
					</tr>
				{{/entity}}
				</tbody>
			</table>
